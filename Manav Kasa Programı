
/**
 * 

package JavaPatikasi;
import java.util.Scanner;
/**
 * @author Eda Patoğlu
 *
 */
public class Manav {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Scanner sc = new Scanner(System.in);
        System.out.println("Armut Kaç Kilo ? :" );
        double armut=sc.nextDouble(); 
        armut=armut*(2.14);
        System.out.println("Elma Kaç Kilo ? :" );
        double elma=sc.nextDouble(); 
        elma=elma*(3.67);
        System.out.println("Domates Kaç Kilo ? :" );
        double domates=sc.nextDouble(); 
        domates=domates*(1.11);
        System.out.println("Muz Kaç Kilo ? :" );
        double muz=sc.nextDouble(); 
        muz=muz*(0.95);
        System.out.println("Patlıcan Kaç Kilo ? :" );
        double patlican=sc.nextDouble(); 
        patlican=patlican*(5);
        double toplam=(armut+elma+domates+muz+patlican);
        System.out.println("Toplam Tutar :"+toplam+" TL" );
        

	}

}
